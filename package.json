{
  "name": "receipts-backend",
  "version": "1.0.0",
  "description": "A Node.js Express API that serves as a proxy to search for police protocol receipts by car number from the Georgian police database",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "pm2:start": "pm2 start ecosystem.config.json --env production",
    "pm2:dev": "pm2 start ecosystem.config.json --env development",
    "pm2:staging": "pm2 start ecosystem.config.json --env staging",
    "pm2:cluster": "pm2 start ecosystem.config.json --only receipts-backend-cluster",
    "pm2:stop": "pm2 stop receipts-backend",
    "pm2:restart": "pm2 restart receipts-backend",
    "pm2:reload": "pm2 reload receipts-backend",
    "pm2:delete": "pm2 delete receipts-backend",
    "pm2:logs": "pm2 logs receipts-backend",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "pm2:save": "pm2 save",
    "pm2:resurrect": "pm2 resurrect",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "api",
    "police",
    "georgia",
    "receipts",
    "proxy",
    "express",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^2.2.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "pm2": "^6.0.13"
  }
}
